    {
      "name": "Authentication",
      "description": "Account registration, login, and profile management endpoints",
      "item": [
        {
          "name": "Register Client",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    var jsonData = pm.response.json();",
                  "    pm.environment.set('jwt_token', jsonData.token);",
                  "    pm.environment.set('userId', jsonData.id);",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/x-www-form-urlencoded"
              }
            ],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "UserName",
                  "value": "john_doe",
                  "description": "Minimum 3 characters",
                  "type": "text"
                },
                {
                  "key": "Email",
                  "value": "john@example.com",
                  "description": "Valid email format",
                  "type": "text"
                },
                {
                  "key": "Password",
                  "value": "SecurePass@123",
                  "description": "Min 8 chars: uppercase, lowercase, digit, special char",
                  "type": "text"
                },
                {
                  "key": "ConfirmPassword",
                  "value": "SecurePass@123",
                  "description": "Must match Password",
                  "type": "text"
                },
                {
                  "key": "FullName",
                  "value": "John Doe",
                  "description": "Minimum 3 characters",
                  "type": "text"
                },
                {
                  "key": "ProfilePhoto",
                  "value": "file_content",
                  "description": "Valid image file",
                  "type": "file"
                },
                {
                  "key": "Role",
                  "value": "1",
                  "description": "1 for Client, 2 for Trainer",
                  "type": "text"
                }
              ]
            },
            "url": {
              "raw": "{{base_url}}/api/account/register",
              "protocol": "https",
              "host": ["{{base_url}}"],
              "path": ["api", "account", "register"]
            }
